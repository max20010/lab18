#include "pch.h"
#include <iostream>
using namespace std;

int main()
{
	setlocale(LC_ALL, "ru");
	int m,h;
	cout << "Введите М (нечетное число): ";
	cin >> m;
	int** a = new int*[m];
	for (int i = 0; i < m; i++) {
		a[i] = new int[m];
	}
	for (int i = 0; i < m; i++) {
		for (int j = 0; j < m; j++) {
			cin >> a[i][j];
		}
	}
	for (int i = 0; i < m; i++) {
		for (int j = 0; j < m; j++) {
			cout << a[i][j] << "\t";
		}
		cout << endl;
	}
	for (int i = 0; i < m; i++) {
		
		for (int j = 0; j < m; j++) {
			cout << a[j][i] << "\t";
		}
		
	}


	cout << endl;
}
