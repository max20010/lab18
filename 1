#include <iostream>
using namespace std;
int main()
{
	setlocale(LC_ALL, "ru");
	int m,y=0,x=0,lim_max,lim_min;
	cin >> m;
	int** a = new int* [m];
	for (int i = 0; i < m; i++) {
		a[i] = new int[m];
	}
	for (int i = 0,l=9; i < m; i++) {
		for (int j = 0; j < m; j++) {
			//cin >> a[i][j];
			l++;
			a[i][j] = l;
		}
	}
	for (int i = 0; i < m; i++) {
		for (int j = 0; j < m; j++) {
			cout << "["<<i<<"]["<<j<<"]"<<a[i][j] << "\t";
		}
		cout << endl;
	}
	lim_max = m;
	lim_min = -1;
	for (y; y < lim_max;y++) {
		cout << a[y][0] << "\t";
	}
	while (a[x][y] != a[(m - 1) / 2][(m - 1) / 2])
	{
		y--;
		x++;
		for (x; x < lim_max;x++) {
			cout << a[y][x] << "\t";
		}
		x--;
		y--;
		lim_min++;
		for (y; y > lim_min;y--) {
			cout << a[y][x]<<"\t";
		}
		for (x; x > lim_min; x--) {
			cout << a[y][x] << "\t";
		}
		x++;
		y++;
		lim_max--;
		for (y; y < lim_max; y++) {
			cout << a[y][x] << "\t";
		}
	}
	delete[]a;
}
